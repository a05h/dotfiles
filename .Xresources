! Render setting for cairo -> pango -> gtk
Xft.dpi:        96
Xft.antialias:  true
Xft.hinting:    true
Xft.rgba:       rgb
Xft.hintstyle:  full

! Urxvt configuration
URxvt.font: xft:ubuntu mono:size=12
URxvt.scrollBar: false
URxvt.letterSpace: 0

URxvt*background: #111219
URxvt*foreground: #c5c8c6

URxvt*color0: #282a2e
URxvt*color1: #a54242
URxvt*color2: #8c9440
URxvt*color3: #de935f
URxvt*color4: #5f819d
URxvt*color5: #85678f
URxvt*color6: #5e8d87
URxvt*color7: #707880

URxvt*color8: #373b41
URxvt*color9: #cc6666
URxvt*color10: #b5bd68
URxvt*color11: #f0c674
URxvt*color12: #81a2be
URxvt*color13: #b294bb
URxvt*color14: #8abeb7
URxvt*color15: #c5c8c6

URxvt.internalBorder: 8
URxvt.geometry: 101x27
URxvt.boldFont:
URxvt*cursorColor: #518FF5

! xrdb ~/.Xresources
